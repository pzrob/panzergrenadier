{game_entity
	(include "/properties/armoredcar.ext")
	(include "/properties/selection/vehicle.inc"  scale(1.0))
	{props "tank" "turret" "wheeled" "8_wheels" "with_gun" "vision_lev01" "muzzle_20mm_25mm_auto"}
	{targetclass "armoredcar"}
	{targetSelector "tank_light"}
	{PatherId "vehicle.small"}
	{patherId "sdkfz232"}	
	{extension "crew.ext"}	
	{Extension "sdkfz232.mdl"}	
	
	{extender "enumerator"
		{digit_folder "ger"}
	}

	{volume "body1"
		{tags "body"}
		{thickness 8
			{front	14.5}
			{top 5.5}
			{bottom 5.5}
			{rear 5.5}			
		}
	}
	{volume "body2"
		{tags "body"}
		{thickness 8
			{front	14.5}
			{top 5.5}
			{bottom 5.5}
			{rear 5.5}			
		}
	}
	{volume "turret"
		{tags "turret"}
		{thickness 8
		    {front 14.5}
			{top 5.5}			
		}
	}

	{volume "engine"
		{thickness 10
			{front 5.5}
			{top 5.5}
			{bottom 5.5}		
		}
	}

	("turret" repair(45))
	("gun" repair(30) thickness(20))	

	{extender "inventory"
		{box
			{item "bulletger_20_auto ap" 50}	; 18 magazines (10 rounds each)
			{item "bulletger_20_auto apcr" 30}
			{item "bulletger_20_auto he" 90}
			{item "ammo mgun_ger belt ap" 1950}	; 14 belts (150 rounds each)
			{item "geballteladung_3kg" 1}
		}
	}
	
	{Weaponry
		("restore_ik_time")
		{place "gun"
			{RestoreIKAfterAim}
			{weapon "20mm_kwk30" filling "bulletger_20_auto ap" 10}
			{gunner "gunner"}
			{charger "charger"}
			("abm_dymamic_autocannon"
				zeroing(2.0)
				dispersion(0.15)
			)
		}
		{place "mgun"
			{type "coaxial"}
			{weapon "mg34_tank" filling "ammo mgun_ger belt ap" 150}
			{foresight "foresight3"}			
			{gunner "gunner"}
			{charger "charger"}
			("abm_mgun")
			{basic}
		}
		{place "mgun3"
			{type "commander"}
			{weapon "binocular"}
			{foresight "foresight5"}
			{gunner "commander"}
			{basic}
			{ai_in_manual}
		}			
	}
	
	("wheel" args "wheelright1")
	("wheel" args "wheelleft1")
	("wheel" args "wheelright2")
	("wheel" args "wheelleft2")
	("wheel" args "wheelright3")
	("wheel" args "wheelleft3")
	("wheel" args "wheelright4")
	("wheel" args "wheelleft4")
	("fuel" args "fuel")	
	("transmission" args "transmission")	

	{mass 8300}
	{Chassis "wheel"
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlf") wheelr("wrf") bonel("FXtraceL1") boner("FXtraceR1"))
		("car_trace" fx("tracewheel03") step(0.08) wheell("wlr") wheelr("wrr") bonel("FXtraceL2") boner("FXtraceR2"))
		("mobility_btr"
			speed(85) ;----------------------------------Max speed.
			speedatmaxturn(25) ;-------------------------Max speed at max turn.
			reverse(85) ;--------------------------------Max speed in Reverse.
			weight(8.3) ;--------------------------------Weight in Tonnes.
			power(180) ;---------------------------------Horse Power.
			fuel(180) ;----------------------------------Fuel capacity in Litres.
			type(fuel) ;---------------------------------Fuel type.
			range(170) ;---------------------------------Range in Kilometers.
		)
	}
}
