	{volume "body1" {thickness 4}}
	{volume "body2" {able {holed}}{thickness 4}}
	{volume "body3" {thickness 4}}
	{volume "engine" {thickness 4}}
	{volume "transmission" {able {NoWoundDecal 0}}}
