		    {item "sandbag_vehicle" 1}
			{item "bulletger_20_auto ap" 140}		; 20 clips (20 rounds each) 33/66 AP/HE
			{item "bulletger_20_auto he" 240}
		}
	}

	{Weaponry
		("restore_ik_time")
